<persistence version="2.1" xmlns="http://xmlns.jcp.org/xml/ns/persistence" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/persistence http://xmlns.jcp.org/xml/ns/persistence/persistence_2_1.xsd">
  <persistence-unit name="java:app/DaaS-Uni" transaction-type="JTA">
    <provider>org.eclipse.persistence.jpa.PersistenceProvider</provider>
    <jta-data-source>java:app/DaaS-Uni</jta-data-source>
    <class>org.dlect.internal.data.LectureContent</class>
    <class>org.dlect.internal.data.LectureData</class>
    <class>org.dlect.internal.data.SemesterData</class>
    <class>org.dlect.internal.data.StreamData</class>
    <class>org.dlect.internal.data.StreamLecture</class>
    <class>org.dlect.internal.data.SubjectData</class>
    <class>org.dlect.internal.data.UniversityData</class>
    <properties/>
  </persistence-unit>
</persistence>
